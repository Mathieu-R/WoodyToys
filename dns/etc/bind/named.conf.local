# /etc/bind/named.conf.local

zone "wt4.ephec-ti.be" IN {

        # Zone de type maître
        type master;

        # Fichier de zone
        file "/etc/bind/wt4.ephec-ti.be/db.wt4.ephec-ti.be";

        # On autorise tout le monde à envoyer des requêtes vers cette zone
        allow-query { any; };

};
